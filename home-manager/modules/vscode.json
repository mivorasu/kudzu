{
    "[typst]": {
        "editor.defaultFormatter": "myriad-dreamin.tinymist"
    },
    "code-runner.enableAppInsights": false,
    "code-runner.runInTerminal": true,
    "code-runner.showExecutionMessage": false,
    "code-runner.showRunCommandInEditorContextMenu": false,
    "code-runner.showRunCommandInExplorerContextMenu": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.autoIndent": "none",
    "editor.codeLensFontFamily": "\"Sarasa Mono SC\",monospace",
    "editor.cursorBlinking": "solid",
    "editor.cursorSmoothCaretAnimation": "off",
    "editor.fontFamily": "\"Sarasa Mono SC\",monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 18,
    "editor.inlineSuggest.showToolbar": "always",
    "editor.inlineSuggest.suppressSuggestions": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.suggestSelection": "first",
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.wordWrap": "on",
    "explorer.confirmDelete": false,
    "explorer.sortOrder": "type",
    "extensions.autoCheckUpdates": false,
    "extensions.autoUpdate": false,
    "extensions.experimental.issueQuickAccess": false,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "justfile": "makefile"
    },
    "files.autoSave": "off",
    "files.exclude": {
        "**/__pycache__": true,
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.venv": true,
        "**/**.exe": true
    },
    "git.confirmSync": false,
    "git.terminalGitEditor": true,
    "github.gitProtocol": "ssh",
    "go.editorContextMenuCommands": {
        "addImport": false,
        "addTags": false,
        "debugTestAtCursor": false,
        "generateTestForFunction": false,
        "playground": false,
        "testAtCursor": false,
        "testCoverage": false,
        "toggleTestFile": false
    },
    "grunt.autoDetect": "on",
    "json.format.keepLines": true,
    "nix.enableLanguageServer": true,
    "nix.formatterPath": "nixfmt",
    "nix.hiddenLanguageServerErrors": [
        "textDocument/definition",
        "textDocument/codeAction",
        "textDocument/documentSymbol",
        "textDocument/inlayHint",
        "textDocument/documentLink"
    ],
    "nix.serverPath": "nixd",
    "nix.serverSettings": {
        "nixd": {
            "formatting": {
                "command": [
                    "nixfmt"
                ]
            }
        }
    },
    "npm.packageManager": "bun",
    "python.createEnvironment.trigger": "off",
    "python.terminal.activateEnvironment": false,
    "redhat.telemetry.enabled": false,
    "roo-cline.allowedCommands": [
        "git log",
        "git diff",
        "git show"
    ],
    "ruff.format.preview": true,
    "ruff.lint.ignore": [
        "E501",
        "TRY301",
        "TRY300",
        "TRY002",
        "PLC0415",
        "PLR2004",
        "RUF001",
        "PLR0914",
        "PLR0915",
        "PLR0912",
        "PLR6301",
        "RUF029",
        "E265"
    ],
    "ruff.lint.preview": true,
    "ruff.lint.select": [
        "E",
        "W",
        "F",
        "B",
        "C4",
        "SIM",
        "RUF",
        "A",
        "RET",
        "G",
        "UP",
        "TRY",
        "PERF",
        "Q",
        "PTH",
        "PL"
    ],
    "ruff.nativeServer": true,
    "search.followSymlinks": false,
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.enabled": false,
    "security.workspace.trust.startupPrompt": "never",
    "security.workspace.trust.untrustedFiles": "open",
    "telemetry.telemetryLevel": "off",
    "terminal.external.linuxExec": "alacritty",
    "terminal.integrated.defaultProfile.linux": "fish",
    "tinymist.compileStatus": "disable",
    "tinymist.formatterMode": "typstyle",
    "tinymist.preview.refresh": "onSave",
    "tinymist.preview.scrollSync": "onSelectionChange",
    "typescript.check.npmIsInstalled": false,
    "update.mode": "none",
    "update.showReleaseNotes": false,
    "window.autoDetectColorScheme": true,
    "window.commandCenter": false,
    "window.dialogStyle": "custom",
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 1.25,
    "workbench.colorTheme": "Quiet Light",
    "workbench.editor.empty.hint": "hidden",
    "workbench.enableExperiments": false,
    "workbench.preferredDarkColorTheme": "Monokai",
    "workbench.preferredLightColorTheme": "Quiet Light",
    "workbench.startupEditor": "none",
    "yaml.extension.recommendations": false
}